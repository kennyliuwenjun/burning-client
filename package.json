{
  "name": "burning-client",
  "version": "0.1.0",
  "homepage": "https://kennyliuwenjun.github.io/burning-client/",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "gh-pages": "^1.2.0",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.4.1",
    "react-datepicker": "^1.5.0",
    "react-dom": "^16.4.1",
    "react-moment": "^0.7.9",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-spinners": "^0.3.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/components -o src/components",
    "watch-css": "npm run build-css && node-sass-chokidar src/components -o src/components --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  }
}
